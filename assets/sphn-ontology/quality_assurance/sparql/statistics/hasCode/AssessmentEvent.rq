        optional{ ?resource sphn:hasAssessment/sphn:hasComponent/sphn:hasResult/sphn:hasQuantity/sphn:hasUnit/sphn:hasCode ?code . }
        optional{ ?resource sphn:hasAssessment/sphn:hasComponent/sphn:hasResult/sphn:hasQuantity/sphn:hasUnit/sphn:hasCode/sphn:hasIdentifier ?code_identifier . }
        optional{ ?resource sphn:hasAssessment/sphn:hasComponent/sphn:hasResult/sphn:hasQuantity/sphn:hasUnit/sphn:hasCode/sphn:hasName ?code_name . }
        optional{ ?resource sphn:hasAssessment/sphn:hasComponent/sphn:hasResult/sphn:hasQuantity/sphn:hasUnit/sphn:hasCode/sphn:hasCodingSystemAndVersion ?code_codingSystemAndVersion . }

    }
    GROUP BY ?origin ?code ?code_identifier ?code_name ?code_codingSystemAndVersion
    }
        UNION
    {
    SELECT ?origin ?code ?code_identifier ?code_name ?code_codingSystemAndVersion (COUNT(?code) as ?count_instances)
    WHERE {
        ?resource a <https://biomedit.ch/rdf/sphn-schema/sphn#AssessmentEvent> .

        BIND("sphn:hasPerformer/sphn:hasCode" as ?origin)
        optional{ ?resource sphn:hasPerformer/sphn:hasCode ?code . }
        optional{ ?resource sphn:hasPerformer/sphn:hasCode/sphn:hasIdentifier ?code_identifier . }
        optional{ ?resource sphn:hasPerformer/sphn:hasCode/sphn:hasName ?code_name . }
        optional{ ?resource sphn:hasPerformer/sphn:hasCode/sphn:hasCodingSystemAndVersion ?code_codingSystemAndVersion . }

    }
    GROUP BY ?origin ?code ?code_identifier ?code_name ?code_codingSystemAndVersion
    }

}