PREFIX sphn:<https://biomedit.ch/rdf/sphn-schema/sphn#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT *
WHERE {
    ?resource a <https://biomedit.ch/rdf/sphn-schema/sphn#Quantity> .
    optional{ ?resource sphn:hasValue ?Value . }
    optional{ ?resource sphn:hasUnit ?Unit . }
    optional{ ?resource sphn:hasUnit/sphn:hasCode ?Unit_Code . }
    optional{ ?resource sphn:hasComparator ?Comparator . }

}