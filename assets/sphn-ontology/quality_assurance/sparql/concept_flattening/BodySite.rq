PREFIX sphn:<https://biomedit.ch/rdf/sphn-schema/sphn#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT *
WHERE {
    ?resource a <https://biomedit.ch/rdf/sphn-schema/sphn#BodySite> .
    optional{ ?resource sphn:hasCode ?Code . }
    optional{ ?resource sphn:hasLaterality ?Laterality . }
    optional{ ?resource sphn:hasLaterality/sphn:hasCode ?Laterality_Code . }

}